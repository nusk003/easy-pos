    "@hm/prettier-config": "^1.33.0-alpha.0",
    "@hm/sdk": "^1.33.0-alpha.0",
    "@hm/spaces": "^1.33.0-alpha.0",
    "@sentry/react": "^5.20.1",
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/react": "^10.4.9",
    "@types/he": "^1.1.1",
    "@types/howler": "^2.1.2",
    "@types/jest": "^25.1.1",
    "@types/lodash": "^4.14.150",
    "@types/markdown-it": "^10.0.3",
    "@types/node": "^13.7.0",
    "@types/qrcode": "^1.4.0",
