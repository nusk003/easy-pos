{
  "name": "@ep/dashboard",
  "version": "1.33.0-alpha.0",
  "main": "./public/electron.js",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build": "craco build",
    "cypress": "cypress open",
    "dev": "node dev/start.js",
    "electron:build": "PUBLIC_URL=./ craco build",
    "electron:dev": "electronmon .",
    "electron:package:mac": "electron-builder -m -c.extraMetadata.main=build/electron.js",
    "electron:package:win": "electron-builder -w -c.extraMetadata.main=build/electron.js",
    "electron:package:linux": "electron-builder -l -c.extraMetadata.main=build/electron.js -p always",
    "eject": "react-scripts eject",
    "start": "craco start",
    "start:https": "HTTPS=true SSL_CRT_FILE=../../../.cert/cert.pem SSL_KEY_FILE=../../../.cert/key.pem craco start",
    "storybook": "start-storybook",
    "test": "CI=true craco test --env=jsdom",
    "test:watch": "craco test --env=jsdom",
    "test:e2e": "wait-on http://localhost:3000 http://localhost:5000 && cypress run"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect",
      "jest-styled-components"
    ]
  },
  "electronVersion": "18.2.3",
  "author": "Hotel Manager",
  "description": "Dashboard from Hotel Manager",
  "build": {
    "appId": "com.hotelmanager.app",
    "productName": "Hotel Manager",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage",
      "category": "Network",
      "publish": {
        "provider": "s3",
        "bucket": "cloud-console-app",
        "region": "eu-west-1"
      }
    },
    "extraMetadata": {
      "name": "Hotel Manager"
    }
  },
  "browserslist": {
    "production": [
      "last 1 electron version",
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 electron version",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "@types/react": "^16.9.35"
  },
  "dependencies": {
    "@babel/helper-call-delegate": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
